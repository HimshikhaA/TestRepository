<aura:component description="temp_FAST_InputPage">

    <lightning:layout multipleRows="true">
        <lightning:layoutItem padding="around-small" size="12">
            <c:FAST_PageTitleComponent pageTitle="{!$Label.c.inputs_CardTitle}"
                    pageSubTitle="{!$Label.c.inputs_Cardtag}"
            />
        </lightning:layoutItem>

        <lightning:layoutItem padding="around-small" size="12">
            <c:FAST_CardComponent cardTitle="{!$Label.c.FAST_PreviewConfigurationLabel}"
                    contentType="dynamicContent"
                    dynamicContent="c:FAST_Preview_FAST_Input"
            />

        </lightning:layoutItem>

    </lightning:layout>
</aura:component>