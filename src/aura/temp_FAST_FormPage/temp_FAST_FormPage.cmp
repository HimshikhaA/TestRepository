<aura:component description="temp_FAST_FormPage">

    <lightning:layout multipleRows="true">
        <lightning:layoutItem padding="around-small" size="12">
            <c:FAST_PageTitleComponent pageTitle="Single Step Form"
                    pageSubTitle="{!$Label.c.forms_tag}"
            />
        </lightning:layoutItem>

        <lightning:layoutItem padding="around-small" size="12">
            <c:FAST_CardComponent cardTitle="{!$Label.c.FAST_PreviewConfigurationLabel}"
                    contentType="dynamicContent"
                    dynamicContent="c:FAST_Preview_FAST_SingleStepForm"
            />

        </lightning:layoutItem>
    </lightning:layout>
</aura:component>