<aura:component description="temp_FAST_AccordionPage">

    <lightning:layout multipleRows="true">
        <lightning:layoutItem padding="around-small" size="12">
            <c:FAST_PageTitleComponent pageTitle="Accordion"
                                       pageSubTitle="Accordions with dynamically generated sections"
            />
        </lightning:layoutItem>

        <lightning:layoutItem padding="around-small" size="12">
            <c:FAST_CardComponent cardTitle="{!$Label.c.FAST_PreviewConfigurationLabel}"
                                  contentType="dynamicContent"
                                  dynamicContent="c:FAST_Preview_FAST_Accordion"
            />

        </lightning:layoutItem>

    </lightning:layout>
</aura:component>